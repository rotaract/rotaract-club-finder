{
	"name": "rotaract/rotaract-club-finder",
	"description": "WordPress plugin for map visualization of rotaract club locations.",
	"type": "project",
	"keywords": [
		"wordpress",
		"plugin",
		"rotaract",
		"store locator"
	],
	"homepage": "https://github.com/rotaract/rotaract-club-finder",
	"readme": "README.adoc",
	"license": "EUPL-1.2",
	"authors": [
		{
			"name": "Rotaract Germany"
		}
	],
	"support": {
		"email": "support@rotaract.de",
		"issues": "https://github.com/rotaract/rotaract-club-finder/issues"
	},
	"require": {
		"php": ">=7.4",
		"elasticsearch/elasticsearch": "^v8.3.3",
		"php-http/guzzle7-adapter": "^1.0",
		"meilisearch/meilisearch-php": "^1.1",
		"guzzlehttp/guzzle": "^7.5",
		"http-interop/http-factory-guzzle": "^1.0"
	},
	"require-dev": {
		"wp-cli/wp-cli-bundle": "^2.5",
		"wp-coding-standards/wpcs": "^3.0.0",
		"phpcompatibility/php-compatibility": "^9.3.5"
	},
	"minimum-stability": "stable",
	"scripts": {
		"lint": "phpcs . --standard=phpcs.xml",
		"translate": "wp-cli i18n make-mo languages"
	},
	"config": {
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"php-http/discovery": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
